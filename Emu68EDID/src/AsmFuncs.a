;----------------------------------------------------------
; 
; Emu68Info assembly functions
; 
; Compiler(s):
; Frank Wille vasm assembler, or
; AmigaOS M68K SAS/C 6.59 Assembler.
; 
;----------------------------------------------------------

;----------------------------------------------------------

	SECTION .text

;----------------------------------------------------------

	XDEF	_asm_nop
	XDEF	_asm_le32

;----------------------------------------------------------
;
; void asm_nop(void)
; 
;----------------------------------------------------------

	cnop 0,4

_asm_nop:
	nop
	rts

;----------------------------------------------------------
;
; void asm_le32(ULONG a)
;               D0
;----------------------------------------------------------

	cnop 0,4

_asm_le32:
	ror.w    #8,d0
	swap     d0
	ror.w    #8,d0
	rts

;----------------------------------------------------------
;
; END OF FILE
;
;----------------------------------------------------------

	end
